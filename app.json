{
  "expo": {
    "name": "Chad PT",
    "slug": "chadgpt",
    "version": "4.0.1",
    "orientation": "portrait",
    "icon": "./assets/icon-chad.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/splash-chad.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.viveksharma.chadgpt"
    },
    "android": {
      "versionCode": 95,
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon-chad.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.viveks_7764.ChadGPT",
      "permissions": ["com.google.android.gms.permission.AD_ID"]
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "extra": {
      "eas": {
        "projectId": "f6f2a0de-e807-40a1-b6fa-f5795a569552"
      }
    },
    "plugins": [
      [
        "expo-build-properties",
        {
          "ios": {
            "useFrameworks": "static"
          }
        }
      ]
    ]
  },
  "react-native-google-mobile-ads": {
    "android_app_id": "ca-app-pub-5578644815986311~7154388442",
    "ios_app_id": "ca-app-pub-5578644815986311~7831339009"
  }
}
