{
  "expo": {
    "name": "Top-G-PT",
    "slug": "Top-G-PT",
    "version": "4.0.4",
    "orientation": "portrait",
    "icon": "./assets/icon-top-g.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/splash-chad.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "versionCode": 98,
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon-top-g.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.viveks_7764.TopGPT",
      "permissions": ["com.google.android.gms.permission.AD_ID"]
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "extra": {
      "eas": {
        "projectId": "dcbb0233-f570-438f-a9df-f7484f3ef967"
      }
    },
    "plugins": [
      [
        "expo-build-properties",
        {
          "ios": {
            "useFrameworks": "static"
          }
        }
      ]
    ]
  },
  "react-native-google-mobile-ads": {
    "android_app_id": "ca-app-pub-5578644815986311~9709752607"
  }
}
